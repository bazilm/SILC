%{
#include<stdlib.h>
#include "y.tab.h"
%}

digits	[0-9]

%%

{digits}+	{yylval.ival = atoi(yytext);
		return INTEGER;
		}

[-+*/;\n]	{return *yytext;}
[a-z]		{yylval.cval = *yytext;
		return VAR;}
==		return EQUALS;
<=		return LE;
>=		return GE;
read		return READ;
write		return WRITE;
exit		return EXIT;
[ \t]		

.		return *yytext;	

			

%%

int yywrap()
{
return 1;
}

							



