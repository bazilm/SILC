decl
	int Func(int &x);
	int x;  
enddecl

int Func(int &x){
	decl
		int t;
	enddecl
	
	begin
		if (x > 90) then
			t = 91;
		else
		    t=x+11;
		    t=Func(t);
		    t=t+1; 
		    t = Func(t);
		endif;
		return t;
	end
}

int main(){
decl
	int result;
enddecl
begin
	read(x);
    result = Func(x);
    write(result);
	return 0;
end
}
